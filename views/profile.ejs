			<div class="row">
				<div class="col">
					<div class="container">
						<div class="row">
                            <div style="min-height: 600px;" class="col">
                                <div class="card-deck col-sm-6"> 
                                <div class="m-2 card">
                                    <div class="card-header"><%= title %></div>
                                    <div class="card-body">
                                        <h5>Welcome <%= user.displayName %>! </h5>
                                        <p><i>Check that your address details are correct </i></p>
                                        <div>
                                        <% if (user._json.address) { %>
                                           
                                                <div> <%= user._json.address.street_address %> </div>
                                                <div> <%= user._json.address.locality %>, <%= user._json.address.region %> </div>
                                                <div> <%= user._json.address.postal_code %> </div>
                                                
                                            <%} else { %>
                                        
                                                <div> 12 Elm St </div>
                                                <div> Whitehorse, YT</div>
                                                <div> Y1A 4A1 </div>
                                            
                                            <% } %>
                                        </div>
                                           
                                    </div>
                                    <div class="card-footer"><span><span><a href="" class="btn btn-secondary">Continue</a></span><span  class="float-right"><a href="" class="btn btn-secondary">Fix my Address</a></span>
                                    </span></div>
                                </div>
                            </div>
                            
                    
